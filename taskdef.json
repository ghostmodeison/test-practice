{
    "family": "Marketplace-Frontend-Task",
    "containerDefinitions": [
        {
            "name": "Marketplace-Frontend-Container",
            "image": "722163322018.dkr.ecr.ap-south-1.amazonaws.com/marketplace-frontend",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "marketplace-frontend-port",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "command": [
                "npm",
                "run",
                "start"
            ],
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/Marketplace-Frontend-Task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::722163322018:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::722163322018:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}